# where to install the binaries
prefix=@prefix@
exec_prefix=@exec_prefix@
BINDIR=@bindir@
LIBDIR=@libdir@

EXE=@EXE@

# where to install the man page
MANDIR=@mandir@

# other variables
OCAMLC   = @OCAMLC@
OCAMLOPT = @OCAMLOPT@
OCAMLDEP = @OCAMLDEP@
OCAMLLEX = @OCAMLLEX@
OCAMLYACC= @OCAMLYACC@
OCAMLLIB = @OCAMLLIB@
OCAMLBEST= @OCAMLBEST@
OCAMLVERSION = @OCAMLVERSION@

INCLUDES = -I src -I c -I intf -I tools
BFLAGS   = -dtypes -g $(INCLUDES) -I +lablgtk2 -I +threads
OFLAGS   = -dtypes $(INCLUDES) -I +lablgtk2 -I +threads

COQC7  = @COQC7@
COQC8  = @COQC8@
COQDEP = @COQDEP@
COQLIB = @COQLIB@

VO7= lib/coq-v7/WhyArrays.vo  lib/coq-v7/WhyBool.vo  lib/coq-v7/WhyInt.vo \
     lib/coq-v7/WhyTuples.vo  lib/coq-v7/WhyPermut.vo \
     lib/coq-v7/WhySorted.vo  lib/coq-v7/Why.vo     lib/coq-v7/WhyFloat.vo \
     lib/coq-v7/WhyExn.vo lib/coq-v7/WhyCoqCompat.vo \
     lib/coq-v7/WhyLemmas.vo  lib/coq-v7/WhyTactics.vo lib/coq-v7/WhyCM.vo 
V7FILES=$(VO7:.vo=.v)

VO8= lib/coq/WhyArrays.vo  lib/coq/WhyBool.vo  lib/coq/WhyInt.vo \
     lib/coq/WhyTuples.vo  lib/coq/WhyPermut.vo lib/coq/WhyPrelude.vo \
     lib/coq/WhySorted.vo  lib/coq/Why.vo     lib/coq/WhyFloat.vo \
     lib/coq/WhyExn.vo lib/coq/WhyCoqCompat.vo \
     lib/coq/WhyLemmas.vo  lib/coq/WhyTactics.vo lib/coq/WhyCM.vo \
     lib/coq/caduceus_why.vo     lib/coq/caduceus_tactics.vo \
     lib/coq/caduceus_lists.vo lib/coq/Caduceus.vo
V8FILES=$(VO8:.vo=.v)

PVSFILES = lib/pvs/why.pvs
PVSLIB = @PVSLIB@

GENERATED = src/version.ml src/lexer.ml src/parser.mli \
            src/parser.ml src/linenum.ml \
	    c/cversion.ml c/clexer.ml c/cparser.ml c/cparser.mli \
	    c/cllexer.ml c/clparser.ml c/clparser.mli c/cpp.ml c/cconst.ml \
	    intf/hilight.ml intf/tagsplit.ml tools/why2html.ml \
	    src/float_lexer.ml tools/cvcl_split.ml tools/simplify_split.ml

# main targets
##############

BINARY=bin/why.$(OCAMLBEST)
CADUCEUS=bin/caduceus.$(OCAMLBEST)
STATICBINARY=bin/why.static
WHY2HTML=bin/why2html.$(OCAMLBEST)
RVMERGE=bin/rv_merge.$(OCAMLBEST)
DP=bin/dp.$(OCAMLBEST)
WHYOBFUSCATOR=bin/why-obfuscator.$(OCAMLBEST)

TOOLS=$(WHY2HTML) $(DP) $(RVMERGE) $(WHYOBFUSCATOR)

all: $(BINARY) $(CADUCEUS) coq-@COQ@ $(TOOLS)

opt: bin/why.opt bin/caduceus.opt

byte : bin/why.byte bin/caduceus.byte

# why
CMO = src/lib.cmo \
      src/version.cmo src/options.cmo src/linenum.cmo src/loc.cmo \
	   src/ident.cmo src/float_lexer.cmo  \
	   src/effect.cmo src/pp.cmo src/misc.cmo 	\
	   src/parser.cmo src/lexer.cmo src/report.cmo \
           src/env.cmo src/rename.cmo src/util.cmo 	\
           src/ltyping.cmo src/typing.cmo src/purify.cmo src/wp.cmo \
           src/monad.cmo      \
	   src/mlize.cmo src/red.cmo src/vcg.cmo \
	   src/monomorph.cmo src/pvs.cmo src/hol4.cmo src/gappa.cmo \
	   src/holl.cmo src/harvey.cmo src/simplify.cmo src/cvcl.cmo \
           src/regen.cmo src/mizar.cmo src/smtlib.cmo src/coq.cmo \
	   src/zenon.cmo tools/calldp.cmo src/dispatcher.cmo \
           src/isabelle.cmo src/ocaml.cmo src/main.cmo
CMX      = $(CMO:.cmo=.cmx)

bin/why.opt: $(CMX) src/why.cmx 
	$(OCAMLOPT) $(OFLAGS) -o $@ unix.cmxa str.cmxa $^
#	strip $@

bin/why.byte: $(CMO) src/why.cmo
	$(OCAMLC) $(BFLAGS) -o $@ unix.cma str.cma $^

bin/why.static: $(CMX) src/why.cmx 
	$(OCAMLOPT) -cclib -static $(OFLAGS) -o $@ unix.cmxa str.cmxa $^
	strip $@

bin/top: $(CMO)
	ocamlmktop $(BFLAGS) -o $@ str.cma $^

# caduceus
CCMO = src/lib.cmo src/linenum.cmo \
	src/loc.cmo src/pp.cmo \
	src/version.cmo c/cversion.cmo c/coptions.cmo \
	c/ctypes.cmo c/info.cmo c/creport.cmo c/cconst.cmo \
	c/cenv.cmo c/cltyping.cmo c/ctyping.cmo c/cprint_graph.cmo \
	c/cgraph.cmo c/cinit.cmo \
	c/cast_misc.cmo c/clparser.cmo c/cllexer.cmo \
	c/cparser.cmo c/clexer.cmo \
	c/output.cmo c/cprint.cmo c/cnorm.cmo  c/cseparation.cmo \
	c/invariant.cmo c/ceffect.cmo \
	c/cinterp.cmo c/cpp.cmo c/cmake.cmo c/cmain.cmo
CCMX = $(CCMO:.cmo=.cmx)

bin/caduceus.opt: $(CCMX)
	$(OCAMLOPT) $(OFLAGS) -o $@ unix.cmxa str.cmxa $^
	strip $@

bin/caduceus.byte: $(CCMO)
	$(OCAMLC) $(BFLAGS) -o $@ unix.cma str.cma $^

bin/caduceus.static: $(CCMX)
	$(OCAMLOPT) -cclib -static $(OFLAGS) -o $@ unix.cmxa str.cmxa $^
	strip $@

# graphical interface
WVCMO = $(CMO) intf/navig.cmo intf/hilight.cmo intf/viewer.cmo
WVCMX = $(WVCMO:.cmo=.cmx)

bin/why-viewer.opt: $(WVCMX)
	$(OCAMLOPT) $(OFLAGS) -o $@ unix.cmxa gramlib.cmxa str.cmxa lablgtk.cmxa $^
	strip $@

bin/why-viewer.byte: $(WVCMO)
	$(OCAMLC) $(BFLAGS) -o $@ unix.cma gramlib.cma str.cma lablgtk.cma $^

# graphical interface
GCMO = $(CMO) intf/colors.cmo intf/tags.cmo intf/tagsplit.cmo intf/astprinter.cmo \
              intf/hilight.cmo intf/pprinter.cmo intf/stat.cmo
GCMX = $(GCMO:.cmo=.cmx)

gwhy: bin/gwhy.$(OCAMLBEST)

bin/gwhy.opt: $(GCMX)
	$(OCAMLOPT) $(OFLAGS) -o $@ unix.cmxa threads.cmxa str.cmxa lablgtk.cmxa gtkThread.cmx $^
	strip $@

bin/gwhy.byte: $(GCMO)
	$(OCAMLC) $(BFLAGS) -o $@ unix.cma str.cma lablgtk.cma $^

# tools
bin/why2html.byte: tools/why2html.ml
	$(OCAMLC) $(BFLAGS) -o $@ $^

bin/why2html.opt: tools/why2html.ml
	$(OCAMLOPT) $(OFLAGS) -o $@ $^

DPCMO = tools/cvcl_split.cmo tools/simplify_split.cmo \
        tools/calldp.cmo tools/dp.cmo
DPCMX = $(DPCMO:.cmo=.cmx)

bin/dp.byte: $(DPCMO)
	$(OCAMLC) $(BFLAGS) -o $@ $^

bin/dp.opt: $(DPCMX)
	$(OCAMLOPT) $(OFLAGS) -o $@ $^

bin/rv_merge.byte: tools/rv_merge.ml
	$(OCAMLC) $(BFLAGS) -o $@ $^

bin/rv_merge.opt: tools/rv_merge.ml
	$(OCAMLOPT) $(OFLAGS) -o $@ $^

OBFCMO=src/pp.cmo src/loc.cmo src/ident.cmo src/float_lexer.cmo \
       src/parser.cmo src/lexer.cmo tools/obfuscator.cmo
OBFCMX=$(OBFCMO:.cmo=.cmx)

bin/why-obfuscator.byte: $(OBFCMO)
	$(OCAMLC) $(BFLAGS) -o $@ $^

bin/why-obfuscator.opt: $(OBFCMX)
	$(OCAMLOPT) $(OFLAGS) -o $@ $^

static:: $(STATICBINARY)

coq-no:
coq-yes: coq-@COQVER@
coq-v7: $(VO7)
coq-v8: $(VO8)

include Version

src/version.ml: Version Makefile
	echo "let coqversion = \""@COQVER@"\"" > $@
	echo "let version = \""$(VERSION)"\"" >> $@
	echo "let date = \""`date`"\"" >> $@
	echo "let libdir = \""$(LIBDIR)/why"\"" >> $@

c/cversion.ml: Version Makefile
	echo "let version = \""$(CVERSION)"\"" > $@
	echo "let date = \""`date`"\"" >> $@
	echo "let libdir = \""$(LIBDIR)/caduceus"\"" >> $@

doc:: doc/version.tex
	cd doc; make

lib/coq/caduceus_why.v: lib/why/caduceus.why $(BINARY)
	WHYLIB=lib/why $(BINARY) --dir lib/coq --coq-v8 lib/why/caduceus.why

lib/coq-v7/caduceus_why.v: lib/why/caduceus.why $(BINARY)
	WHYLIB=lib/why $(BINARY) --dir lib/coq-v7 --coq-v7 lib/why/caduceus.why

# bench

.PHONY: bench test

WHYVO=lib/coq/Why.vo

bench:: $(BINARY) $(WHYVO)
	cd bench; sh ./bench "../$(BINARY)"

bench-c:: $(CADUCEUS) $(WHYVO)
	cd bench/c; sh ./bench

bench-pvs:: $(BINARY) $(WHYVO)
	cd bench; sh ./bench "../$(BINARY) --valid" pvs

bench-tc:: $(BINARY) $(WHYVO)
	cd bench; sh ./bench "../$(BINARY) -tc"

test:: $(BINARY) $(WHYVO)
	$(BINARY) -d -V -coq bench/test.ml

examples:: $(BINARY) $(WHYVO)
	make -C examples

examples-c:: $(BINARY) $(CADUCEUS) $(WHYVO)
	make -C examples-c

# debugging

db debug: bin/why.byte src/logic.cmo


float f_single(int32 n)
  requires (C_18 : ((C_19 : (0 <= n)) && (C_20 : (n <= 1000000))));
behavior default:
  ensures (C_17 : (\real_abs(((\result :> real) - (\at(n,Old) * 0.1))) <=
                    (\at(n,Old) * (0x1p-8 + 1.49012e-09))));
{  
   (var float t);
   
   (var int32 i);
   
   {  (C_1 : (t = (0.0 :> float)));
      (C_2 : (i = 0));
      
      loop 
      behavior default:
        invariant (C_5 : ((C_6 : (0 <= i)) && (C_7 : (i <= n))));
      behavior default:
        invariant (C_4 : (\real_abs(((t :> real) - (i * 0.1))) <=
                           (i * (0x1p-8 + 1.49012e-09))));
      variant (C_3 : (n - i));
      while (true)
      {  
         {  (if (i < n) then () else 
            (goto while_0_break));
            
            {  (L : 
               {  
                  {  
                     (assert for default: (C_8 : ((C_9 : (0.0 <= (t :> real))) &&
                                                   (C_10 : ((t :> real) <=
                                                             (1000000.0 *
                                                               (0.1 +
                                                                 (0x1p-8 +
                                                                   1.49012e-09))))))));
                     ()
                  };
                  (C_12 : (t = (C_11 : (t + (0.1 :> float)))))
               });
               
               {  
                  (assert for default: (C_13 : (\real_abs(((t :> real) -
                                                            ((\at(t,L) :> real) +
                                                              ((0.1 :> float) :> real)))) <=
                                                 0x1p-8)));
                  ()
               }
            };
            (C_16 : (i = (C_15 : ((C_14 : (i + 1)) :> int32))))
         }
      };
      (while_0_break : ());
      
      (return t)
   }

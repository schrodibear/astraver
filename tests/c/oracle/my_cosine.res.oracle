lemma method_error :
(\forall real x;
  ((\real_abs(x) <= 0x1p-5) ==>
    (\real_abs(((1.0 - ((x * x) * 0.5)) - \cos(x))) <= 0x1p-24)))

float my_cos1(float x)
  requires (C_7 : (\real_abs((x :> real)) <= 0x1p-5));
behavior default:
  ensures (C_6 : (\real_abs(((\result :> real) - \cos((\at(x,Old) :> real)))) <=
                   0x1p-23));
{  
   (var float __retres);
   
   {  
      {  
         (assert for default: (C_1 : (jessie : (\real_abs(((1.0 -
                                                             (((x :> real) *
                                                                (x :> real)) *
                                                               0.5)) -
                                                            \cos((x :> real)))) <=
                                                 0x1p-24))));
         ()
      };
      
      {  (C_5 : (__retres = (C_4 : ((1.0 :> float) -
                                     (C_3 : ((C_2 : (x * x)) *
                                              (0.5 :> float)))))));
         
         (return __retres)
      }
   }
}


axiom Permut_refl:
  (forall int_P_int_M_a_0_2_at_L:(int_P, int) memory.
    (forall a_1:int_P pointer.
      (forall l_0:int.
        (forall h_0:int. Permut(a_1, l_0, h_0, int_P_int_M_a_0_2_at_L,

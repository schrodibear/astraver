
//# AnnotationPolicy = WeakPre
# AnnotationPolicy = Invariants
# AbstractDomain = Box

type intP = {
    integer intM;
}

logic integer f(intP[0] a) reads \nothing;

unit func(intP[0] i, intP[0] j) {
    assert (0 < f(j));
    assert (0 > f(i));
    i = j;
    assert (f(i) > 0);
    assert (f(j) > 0);
}



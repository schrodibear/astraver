
# AnnotationPolicy = WeakPre
# AbstractDomain = Oct

integer foo(integer s, integer n) {
    integer i = 0;
    while true ; {
        if (i < n) {
            assert (0 <= i && i < s);
        }
        i = i + 1;
    }
    return i;
}


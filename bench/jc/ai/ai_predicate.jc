
# AnnotationPolicy = Invariants
# AbstractDomain = Box

logic pos(integer x) = x > 0


unit f(integer n) 
  requires pos(n); 
{
   integer i = n;
   variant 10 - i; 
   while (i < 10) {
       integer j = 1 / i;
       i++;
   }
}

unit f_bis(integer n) 
  requires n > 0; 
{
   integer i = n;
   variant 10 - i; 
   while (i < 10) {
       integer j = 1 / i;
       i++;
   }
}

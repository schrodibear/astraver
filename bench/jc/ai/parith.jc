
# AnnotationPolicy = WeakPre
# AbstractDomain = Pol

type char = {
  integer val;
}

unit f(char[0..10] p) {
  char[..] q = p;
  integer i = 0;
  invariant true; 
  // 0 <= i && i <= 10 && \offset_min(q) <= 0 && \offset_max(q) >= 10 - i;
  variant 10 - i;
  while (i <= 9) 
  {
      q.val = 0;
      q = q + 1;
      i++;
  }	
}	


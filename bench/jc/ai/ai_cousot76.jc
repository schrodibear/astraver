
/* example from Cousot's 1976 paper */

# AnnotationPolicy = Invariants
# AbstractDomain = Box

/* inferred invariant: 1 <= i && i <= 101;
 * inferred postcondition: \result == 101;
 */

integer cousot76() {
    integer i = 1;
    while (i <= 100) {
	i++;
    }
    return i;
}



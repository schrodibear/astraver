
/* example from Cousot's 1976 paper */

/*
@OPTIONS: -ai box
*/

/* inferred invariant: 1 <= i && i <= 101;
 * inferred postcontion: \result == 101;
 */

integer cousot76() {
    integer i = 1;
    while(i <= 100);
    {
        i++;
    }
    //@ assert i == 101;
    return i;
}




# SeparationPolicy = Regions

type intP = {
    integer intM;
}

integer f() {
    intP[..] tab = new intP[10];
    return tab.intM;
}

intP[0] myalloc() {
    return new intP[1];
}

unit g() {
    intP[0] p = myalloc();
    intP[0] q = myalloc();
    p.intM = 0;
    q.intM = 1;
    assert(p.intM == 0 && q.intM == 1);
}



//# AnnotationPolicy = WeakPre
# AnnotationPolicy = Invariants
# AbstractDomain = Oct

integer foo(integer s, integer n) 
    requires n >= 0;
{
    var integer i = 0;
    while (true) {
        assert (0 <= i && i < s);
        i = i + 1;
	if (i >= n) then return i;
    }
}


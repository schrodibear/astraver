
//# AnnotationPolicy = WeakPre
# AnnotationPolicy = Invariants
# AbstractDomain = Oct

type intP = {
    integer intM;
}

integer bar(intP[..] p, integer n) {
    var integer i = 0;
    while (true) {
        if (i < n) then {
	    (p + i).intM = 0;
        };
        i = i + 1;
    };
    return i;
}


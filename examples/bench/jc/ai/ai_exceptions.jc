# InvariantPolicy = Arguments
# AnnotationPolicy = Invariants
# AbstractDomain = Oct

exception E of unit

logic integer i

unit f0() {
  var integer tmp = i;
  if (i < 0) then throw E (); 
}


unit g()
  behavior exceptional:
    throws E;
    ensures true;
;

unit f1() {
  var integer tmp = i;
  if (i < 0) then g(); 
}


integer h()
  behavior normal:
    ensures \result >= 0;
;

unit f2() {
  var integer tmp = i;
  if (tmp != h()) then g();
}


unit f3() {
   throw E ();
   throw E ();
}

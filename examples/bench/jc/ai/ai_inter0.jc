
# AnnotationPolicy = Invariants
# AbstractDomain = Box

/* 
@OPTIONS: -main main 
*/


type intM = { integer val; }


unit f(intM[0..] t) {
    (t + 1).val = 0;
}

unit main() {
   var intM[0..] t1 = new intM[5];
   f(t1);
   var intM[0..] t2 = new intM[10];
   f(t2);
}


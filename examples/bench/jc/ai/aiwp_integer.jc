
//# AnnotationPolicy = WeakPre
# AnnotationPolicy = Invariants
# AbstractDomain = Oct

integer foo(integer s, integer n) {
    var integer i = 0;
    while (true) {
        if (i < n) then {
            assert (0 <= i && i < s);
	};
        i = i + 1;
    };
    return i;
}


ASSERT (FORALL (t:(ARRAY INT OF INT)):
       (FORALL (i:INT):
       (FORALL (j:INT):
       (FORALL (v:INT):
       ((i /= j) => (access_int(update_int(t, i, v), j) = access_int(t, j)))))));


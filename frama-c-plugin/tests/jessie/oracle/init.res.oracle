  S_alloc_table: S alloc_table, int_P_alloc_table: int_P alloc_table,
  S_b: (S, int_P pointer) memory) =
  ((offset_min(S_alloc_table, p) <= a) and
   left_valid_struct_int_P(select(S_b, p), 0, int_P_alloc_table))

